<h1>Editing notebook_link</h1>

<%= error_messages_for :notebook_link %>

<% form_for(@notebook_link) do |f| %>
  <p>
    <b>Political notebook</b><br />
    <%= f.text_field :political_notebook_id %>
  </p>

  <p>
    <b>Url</b><br />
    <%= f.text_field :url %>
  </p>

  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Date</b><br />
    <%= f.text_field :date %>
  </p>

  <p>
    <b>Source</b><br />
    <%= f.text_field :source %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <b>Is internal</b><br />
    <%= f.check_box :is_internal %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @notebook_link %> |
<%= link_to 'Back', notebook_links_path %>
